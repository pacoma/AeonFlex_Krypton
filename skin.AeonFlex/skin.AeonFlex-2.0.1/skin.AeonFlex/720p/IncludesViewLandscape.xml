		</control>
		<control type="image">
			<left>455</left>
			<width>156</width>
			<height>235</height>
			<aspectratio scalediffuse="true">stretch</aspectratio>
			<texture fallback="resource://resource.images.moviegenrefanart.flex/moviegenres_poster/default.jpg" background="true">$VAR[art_value_poster]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image">
			<description>schatten rechts</description>
			<left>455</left>
			<height>235</height>
			<include>global_texture_border5_rightv</include>
		</control>
		<control type="image">
			<description>schatten rechts</description>
			<left>611</left>
			<height>235</height>
			<include>global_texture_border5_rightv</include>
		</control>
		<control type="label">
			<left>625</left>
			<top>15</top>
			<width>350</width>
			<height>22</height>
			<label>[COLOR=TextColor2]$LOCALIZE[135] [/COLOR] $INFO[ListItem.Label]</label>
			<font>SemiNormal</font>
		</control>
		<control type="fixedlist"><!-- Content liste -->
			<viewtype>list</viewtype>
			<left>625</left>
			<top>45</top>
			<width>555</width>
			<height>187</height>
			<onleft>50</onleft><!-- DB list -->
			<onright>50</onright><!-- DB list -->
			<onup>noop</onup>
			<ondown>noop</ondown>
			<focusposition>0</focusposition>
			<orientation>vertical</orientation>
			<scrolltime>450</scrolltime>
			<itemlayout width="555" height="30">
				<control type="label">
					<width>555</width>
					<height>30</height>
					<textcolor>TextColor2</textcolor>
					<label>$INFO[ListItem.Label]$INFO[ListItem.Episode, (,)]</label>
				</control>
			</itemlayout>
			<focusedlayout width="555" height="30">
				<control type="label">
					<width>555</width>
					<height>30</height>
					<textcolor>TextColor2</textcolor>
					<label>$INFO[ListItem.Label]$INFO[ListItem.Episode, (,)]</label>
				</control>
			</focusedlayout>
			<content target="video">$VAR[content_focus]</content>
			<onclick condition="String.IsEmpty(ListItem.Duration)">ActivateWindow(videos,&quot;videodb://tvshows/titles/$INFO[ListItem.DBID]/&quot;)</onclick>
