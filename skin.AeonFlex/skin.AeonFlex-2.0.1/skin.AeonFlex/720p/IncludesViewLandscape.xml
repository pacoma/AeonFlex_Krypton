<include name="viewlandscapeex-topelements">
	<control type="group">
		<visible>!Container.Content(genres)</visible>
		<control type="image"><!-- artwork wildcard1-->
			<width>455</width>
			<height>235</height>
			<aspectratio scalediffuse="true" align="right" aligny="center">scale</aspectratio>
			<texture background="true" diffuse="artworks/Infopanel_square_diffuse.png">$VAR[viewlandscapeex-value-wildcard1-thumbartwork]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image"><!-- clarartwork wildcard1-->
			<left>100</left>
			<width>355</width>
			<height>235</height>
			<texture colordiffuse="ACBackgroundColorClearart" diffuse="artworks/Infopanel_square_clearart_bg.png">$VAR[viewlandscapeex-value-wildcard1-cleardiffuse]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image">
			<width>455</width>
			<height>235</height>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
			<aspectratio align="right" aligny="center">keep</aspectratio>
			<bordersize>32</bordersize>
			<texture background="true">$VAR[viewlandscapeex-value-wildcard1-clearartwork]</texture>
		</control>

		<control type="image"><!-- artwork wildcard2-->
			<left>455</left>
			<width>370</width>
			<height>235</height>
			<aspectratio scalediffuse="true" align="center" aligny="center">scale</aspectratio>
			<texture background="true">$VAR[viewlandscapeex-value-wildcard2-thumbartwork]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image"><!-- clarartwork wildcard2-->
			<left>455</left>
			<width>370</width>
			<height>235</height>
			<texture colordiffuse="ACBackgroundColorClearart" diffuse="artworks/Infopanel_square_clearart_bg_flip.png">$VAR[viewlandscapeex-value-wildcard2-cleardiffuse]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image">
			<left>455</left>
			<width>370</width>
			<height>235</height>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
			<aspectratio align="center" aligny="center">keep</aspectratio>
			<bordersize>32</bordersize>
			<texture background="true">$VAR[viewlandscapeex-value-wildcard2-clearartwork]</texture>
		</control>
		
		<control type="image"><!-- artwork wildcard3-->
			<left>825</left>
			<width>455</width>
			<height>235</height>
			<aspectratio scalediffuse="true" align="left" aligny="center">scale</aspectratio>
			<texture background="true" diffuse="artworks/Infopanel_square_diffuse_flip.png">$VAR[viewlandscapeex-value-wildcard3-thumbartwork]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image"><!-- clarartwork wildcard3-->
			<left>825</left>
			<width>455</width>
			<height>235</height>
			<texture colordiffuse="ACBackgroundColorClearart" flipx="true" diffuse="artworks/Infopanel_square_clearart_bg.png">$VAR[viewlandscapeex-value-wildcard3-cleardiffuse]</texture>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
		</control>
		<control type="image">
			<left>825</left>
			<width>455</width>
			<height>235</height>
			<fadetime>PanelartworksCrossFadeTime</fadetime>
			<aspectratio align="right" aligny="center">keep</aspectratio>
			<bordersize>32</bordersize>
			<texture background="true">$VAR[viewlandscapeex-value-wildcard3-clearartwork]</texture>
		</control>
		<control type="grouplist"><!-- Info -->
			<left>100</left>
			<top>19</top>
			<orientation>vertical</orientation>
			<itemgap>5</itemgap>
			<include>viewglobal_topelements_textinfo</include>
			<visible>String.Contains(Skin.String(LandscapeEXRow1),Info) | String.Contains(Skin.String(LandscapeEXRow2),Info) | String.Contains(Skin.String(LandscapeEXRow3),Info)</visible>
			<animation effect="slide" end="375" time="0" reversible="false" condition="String.Contains(Skin.String(LandscapeEXRow2),Info)">Conditional</animation>
			<animation effect="slide" end="745" time="0" reversible="false" condition="String.Contains(Skin.String(LandscapeEXRow3),Info)">Conditional</animation>
		</control>
		<control type="textbox"><!-- plot -->
			<top>19</top>
			<left>100</left>
			<width>320</width>
			<include>PlotNormal176</include>
			<include>viewglobal_topelements_plot</include>
			<visible>String.IsEmpty(Skin.String(LandscapeEXRow2)) | String.Contains(Skin.String(LandscapeEXRow1),Plot) | String.Contains(Skin.String(LandscapeEXRow2),Plot) | String.Contains(Skin.String(LandscapeEXRow3),Plot)</visible>
			<animation effect="slide" end="375" time="0" reversible="false" condition="String.IsEmpty(Skin.String(LandscapeEXRow2)) | String.Contains(Skin.String(LandscapeEXRow2),Plot)">Conditional</animation>
			<animation effect="slide" end="745" time="0" reversible="false" condition="String.Contains(Skin.String(LandscapeEXRow3),Plot)">Conditional</animation>
		</control>
		<control type="group"><!-- content -->
			<left>100</left>
			<top>15</top>
			<visible>String.Contains(Skin.String(LandscapeEXRow1),Content) | String.Contains(Skin.String(LandscapeEXRow2),Content) | String.Contains(Skin.String(LandscapeEXRow3),Content)</visible>
			<animation effect="slide" end="365" time="0" reversible="false" condition="String.Contains(Skin.String(LandscapeEXRow2),Content)">Conditional</animation>
			<animation effect="slide" end="755" time="0" reversible="false" condition="String.Contains(Skin.String(LandscapeEXRow3),Content)">Conditional</animation>
			<control type="label">
				<left>5</left>
				<width>350</width>
				<height>22</height>
				<label>[COLOR=TextColor2]$LOCALIZE[31088]:[/COLOR] $VAR[videocontent_similarlabel]</label>
				<font>SemiNormal</font>
			</control>
			<control type="fixedlist" id="5366">
				<viewtype>list</viewtype>
				<top>30</top>
				<width>350</width>
				<height>157</height>
				<onleft>50</onleft>
				<onright>50</onright>
				<onup>5366</onup>
				<ondown>5366</ondown>
				<autoscroll time="3000" delay="14000" repeat="5000">false</autoscroll>
				<focusposition>0</focusposition>
				<orientation>vertical</orientation>
				<scrolltime>450</scrolltime>
				<itemlayout width="350" height="30">
					<control type="label">
						<left>5</left>
						<width>350</width>
						<height>30</height>
						<textcolor>TextColor2</textcolor>
						<label>$INFO[ListItem.Label]$INFO[ListItem.Episode, (,)]</label>
					</control>
				</itemlayout>
				<focusedlayout width="350" height="30">
					<control type="image">
						<width>350</width>
						<height>30</height>
						<include>global_listdiffusefocustexture_flip</include>
						<visible>Control.HasFocus(5366)</visible>
					</control>
					<control type="label">
						<left>5</left>
						<width>350</width>
						<height>30</height>
						<textcolor>TextColor2</textcolor>
						<label>$INFO[ListItem.Label]$INFO[ListItem.Episode, (,)]</label>
						<visible>!Control.HasFocus(5366)</visible>
					</control>
					<control type="label">
						<left>5</left>
						<width>350</width>
						<height>30</height>
						<font>RegularNormal</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>TextColorFocus</textcolor>
						<selectedcolor>TextColorSelectFO</selectedcolor>
						<label>$INFO[ListItem.Label]$INFO[ListItem.Episode, (,)]</label>
						<visible>Control.HasFocus(5366)</visible>
					</control>
				</focusedlayout>
				<content limit="$VAR[videocontent_similar_limit]" sortby="$VAR[videocontent_similar_sort]" target="video">$VAR[videocontent_similar]</content>
				<onclick condition="String.IsEqual(ListItem.DBTYPE,movie)">PlayMedia("$INFO[ListItem.FilenameandPath]")</onclick><!-- check -->
				<onclick condition="String.IsEqual(ListItem.DBTYPE,tvshow)">ActivateWindow(videos,&quot;videodb://tvshows/titles/$INFO[ListItem.DBID]/&quot;)</onclick><!-- check -->
			</control>
		</control>
	</control>
	<!-- genre fix -->
	<control type="group">
		<left>60</left>
		<visible>!ListItem.IsParentFolder + Container.Content(genres)</visible>
		<!-- Fanart -->
		<control type="image">
			<width>455</width>
			<height>235</height>
			<aspectratio scalediffuse="true" align="left" aligny="center">scale</aspectratio>
