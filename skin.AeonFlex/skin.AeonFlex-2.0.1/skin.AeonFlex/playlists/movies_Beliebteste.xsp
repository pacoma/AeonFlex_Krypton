<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Filme - Sortierung Playcount</name>
    <match>all</match>
    <rule field="playcount" operator="greaterthan">
        <value>1</value>
    </rule>
    <limit>12</limit>
    <order direction="descending">playcount</order>
</smartplaylist>
