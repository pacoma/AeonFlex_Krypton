<?xml version="1.0" encoding="UTF-8"?>
<overrides>
	<doNotShareMenu />
	<override action="globaloverride">
		<action>Dialog.Close(all)</action>
		<action>::ACTION::</action>
	</override>
	<contextmenu>
		<enableon>211</enableon>
		<item control="401">Choose item</item>
		<item control="301">Add</item>
		<item control="302">Delete</item>
		<item control="303">Move up</item>
		<item control="304">Move down</item>
		<item control="313" condition="String.IsEqual(Container(211).ListItem.Property(skinshortcuts-disabled),False)">Disable</item>
		<item control="313" condition="String.IsEqual(Container(211).ListItem.Property(skinshortcuts-disabled),True)">Enable</item>
	</contextmenu>
	<alwaysRestore>True</alwaysRestore>
</overrides>